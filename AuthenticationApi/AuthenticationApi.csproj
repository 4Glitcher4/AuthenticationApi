    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <InvariantGlobalization>true</InvariantGlobalization>
    <UserSecretsId>f7382fc0-e905-421a-bdcd-d8b54027ee63</UserSecretsId>
